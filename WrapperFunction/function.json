{
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "authLevel": "function",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": [
          "GET","POST","DELETE","HEAD","PATCH","PUT","OPTIONS"
        ],
        "route": "/{*route}"
      },
      {
        "type": "http",
        "direction": "out",
        "name": "$return"
      }
    ]
  }